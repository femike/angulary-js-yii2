{
    "name": "angulary-js-yii2",
    "description": "Demo angulary-js-yii2",
    "keywords": ["yii2", "framework", "demo", "angulary"],
    "homepage": "https://github.com/femike/angulary-js-yii2",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mikhail Samokhvalov",
            "email": "femike@mail.ru",
            "homepage": "http://ang-yii2.unixoct.ru/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "components/jquery": "dev-master",
        "angular-ui/bootstrap": "dev-master"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\InstallHandler::setPermissions"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/jquery",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/femike/first",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "extra": {
        "writable": [
            "frontend/runtime",
            "frontend/web/assets"
        ],
        "executable": [
            "yii"
        ]
    }
}
